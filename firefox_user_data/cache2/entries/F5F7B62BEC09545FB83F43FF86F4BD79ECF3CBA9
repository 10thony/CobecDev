//Javascript Code
//Password Checker
//Check fields
function checkField(myfield) {
    var element = document.getElementById(myfield);

    if (element.value !== '') {
        element.classList.remove("is-invalid")
    }
}

$(function () {
    "user strict";

    // start password checking logic
    password.init();

});


// Hold functions related to client side password validation
var password = (function () {
    "use strict";

    var minPasswordLength = 8;

    var _hasLowerCase = /[a-z]/;
    var _hasUpperCase = /[A-Z]/;
    var _hasDigit = /\d/;
    // match special characters except space
    var _hasSpecial = /(_|[^\w\d ])/;

    var password = [];
    var password2 = [];

    var hasEightCharsListItem,
        hasUpperCaseListItem,
        hasLowerCaseListItem,
        hasSpecialListItem,
        hasDigitListItem;

    // Enforces that password2 = password, and display an error message if it does not
    var mustMatch = function () {

        if (password.val() !== password2.val()) {
            // show not matching error
            password2.addClass('invalid-field');
            document.getElementById("PW_NoMatch").style.display = "block";
            return false;
        }

        password2.removeClass('invalid-field');
        document.getElementById("PW_NoMatch").style.display = "none";
        return true;
    };

    // check validation and adjust classes
    var checkAndSwitchClasses = function (has, $element) {
        if (has) {
            $element.find(".fa-solid").removeClass("fa-solid fa-circle-xmark").removeClass('invalid').addClass('valid').addClass("fa-solid fa-circle-check");
            return true;
        }

        $element.find(".fa-solid").removeClass("fa-solid fa-circle-check").removeClass('valid').addClass('invalid').addClass("fa-solid fa-circle-xmark");
        return false;

    };

    // Enforces server side password rules on the client for convenience
    var enforceRules = function () {

        $('.invalid').removeClass('invalid');

        var pw = password.val().toLowerCase();

        var hasEight = pw.length >= minPasswordLength;
        var hasLower = _hasLowerCase.test(password.val());
        var hasUpper = _hasUpperCase.test(password.val());
        var hasDigit = _hasDigit.test(password.val());
        var hasSpecial = _hasSpecial.test(password.val());

        checkAndSwitchClasses(hasEight, hasEightCharsListItem);
        checkAndSwitchClasses(hasLower, hasLowerCaseListItem);
        checkAndSwitchClasses(hasUpper, hasUpperCaseListItem);
        checkAndSwitchClasses(hasDigit, hasDigitListItem);
        checkAndSwitchClasses(hasSpecial, hasSpecialListItem);

        if (hasEight == true) {
            document.getElementById("PWC_01_Msg_g").style.display = 'inline';
            document.getElementById("PWC_01_Msg_b").style.display = 'none';
        } else {
            document.getElementById("PWC_01_Msg_g").style.display = 'none';
            document.getElementById("PWC_01_Msg_b").style.display = 'inline';
        }

        if (hasLower == true) {
            document.getElementById("PWC_03_Msg_g").style.display = 'inline';
            document.getElementById("PWC_03_Msg_b").style.display = 'none';
        } else {
            document.getElementById("PWC_03_Msg_g").style.display = 'none';
            document.getElementById("PWC_03_Msg_b").style.display = 'inline';
        }

        if (hasUpper == true) {
            document.getElementById("PWC_02_Msg_g").style.display = 'inline';
            document.getElementById("PWC_02_Msg_b").style.display = 'none';
        } else {
            document.getElementById("PWC_02_Msg_g").style.display = 'none';
            document.getElementById("PWC_02_Msg_b").style.display = 'inline';
        }

        if (hasDigit == true) {
            document.getElementById("PWC_04_Msg_g").style.display = 'inline';
            document.getElementById("PWC_04_Msg_b").style.display = 'none';
        } else {
            document.getElementById("PWC_04_Msg_g").style.display = 'none';
            document.getElementById("PWC_04_Msg_b").style.display = 'inline';
        }

        if (hasSpecial == true) {
            document.getElementById("PWC_05_Msg_g").style.display = 'inline';
            document.getElementById("PWC_05_Msg_b").style.display = 'none';
        } else {
            document.getElementById("PWC_05_Msg_g").style.display = 'none';
            document.getElementById("PWC_05_Msg_b").style.display = 'inline';
        }

        if (pw.length === 0) $('.invalid').removeClass('invalid');

        if (mustMatch() && (hasEight && hasLower && hasUpper && hasDigit && hasSpecial)) {
            document.getElementById('btnAccount').disabled = false;
        } else {
            document.getElementById('btnAccount').disabled = true;
        }
    };

    return {

        init: function () {
            // hook all password/password2 fields on a page
            password = $('#password');
            password2 = $('#passwordverify');

            // hook all req list items
            hasEightCharsListItem = $('#req-length');
            hasUpperCaseListItem = $('#req-upper');
            hasLowerCaseListItem = $('#req-lower');
            hasSpecialListItem = $('#req-special');
            hasDigitListItem = $('#req-digit');

            password.keyup(enforceRules);
            password2.keyup(enforceRules);

        }
    };
}());



//Password Checker after post back

function CheckAgain() {

    var minPasswordLength = 8;

    var _hasLowerCase = /[a-z]/;
    var _hasUpperCase = /[A-Z]/;
    var _hasDigit = /\d/;

    var _hasSpecial = /(_|[^\w\d ])/;

    var password = [];
    var password2 = [];

    var hasEightCharsListItem,
        hasUpperCaseListItem,
        hasLowerCaseListItem,
        hasSpecialListItem,
        hasDigitListItem;

    // Enforces that password2 = password, and display an error message if it does not
    var mustMatch = function () {

        if (password.val() !== password2.val()) {
            // show not matching error
            password2.addClass('invalid-field');
            document.getElementById("PW_NoMatch").style.display = "block";
            return false;
        }

        password2.removeClass('invalid-field');
        document.getElementById("PW_NoMatch").style.display = "none";
        return true;
    };

    // check validation and adjust classes
    var checkAndSwitchClasses = function (has, $element) {
        if (has) {
            $element.find(".fa-solid").removeClass("fa-solid fa-circle-xmark").removeClass('invalid').addClass('valid').addClass("fa-solid fa-circle-check");
            return true;
        }

        $element.find(".fa-solid").removeClass("fa-solid fa-circle-check").removeClass('valid').addClass('invalid').addClass("fa-solid fa-circle-xmark");
        return false;

    };

    password = $('#password');
    password2 = $('#passwordverify');

    // hook all req list items
    hasEightCharsListItem = $('#req-length');
    hasUpperCaseListItem = $('#req-upper');
    hasLowerCaseListItem = $('#req-lower');
    hasSpecialListItem = $('#req-special');
    hasDigitListItem = $('#req-digit');

    $('.invalid').removeClass('invalid');

    var pw = password.val().toLowerCase();

    var hasEight = pw.length >= minPasswordLength;
    var hasLower = _hasLowerCase.test(password.val());
    var hasUpper = _hasUpperCase.test(password.val());
    var hasDigit = _hasDigit.test(password.val());
    var hasSpecial = _hasSpecial.test(password.val());

    checkAndSwitchClasses(hasEight, hasEightCharsListItem);
    checkAndSwitchClasses(hasLower, hasLowerCaseListItem);
    checkAndSwitchClasses(hasUpper, hasUpperCaseListItem);
    checkAndSwitchClasses(hasDigit, hasDigitListItem);
    checkAndSwitchClasses(hasSpecial, hasSpecialListItem);

    if (hasEight == true) {
        document.getElementById("PWC_01_Msg_g").style.display = 'inline';
        document.getElementById("PWC_01_Msg_b").style.display = 'none';
    } else {
        document.getElementById("PWC_01_Msg_g").style.display = 'none';
        document.getElementById("PWC_01_Msg_b").style.display = 'inline';
    }

    if (hasLower == true) {
        document.getElementById("PWC_03_Msg_g").style.display = 'inline';
        document.getElementById("PWC_03_Msg_b").style.display = 'none';
    } else {
        document.getElementById("PWC_03_Msg_g").style.display = 'none';
        document.getElementById("PWC_03_Msg_b").style.display = 'inline';
    }

    if (hasUpper == true) {
        document.getElementById("PWC_02_Msg_g").style.display = 'inline';
        document.getElementById("PWC_02_Msg_b").style.display = 'none';
    } else {
        document.getElementById("PWC_02_Msg_g").style.display = 'none';
        document.getElementById("PWC_02_Msg_b").style.display = 'inline';
    }

    if (hasDigit == true) {
        document.getElementById("PWC_04_Msg_g").style.display = 'inline';
        document.getElementById("PWC_04_Msg_b").style.display = 'none';
    } else {
        document.getElementById("PWC_04_Msg_g").style.display = 'none';
        document.getElementById("PWC_04_Msg_b").style.display = 'inline';
    }

    if (hasSpecial == true) {
        document.getElementById("PWC_05_Msg_g").style.display = 'inline';
        document.getElementById("PWC_05_Msg_b").style.display = 'none';
    } else {
        document.getElementById("PWC_05_Msg_g").style.display = 'none';
        document.getElementById("PWC_05_Msg_b").style.display = 'inline';
    }

    if (pw.length === 0) $('.invalid').removeClass('invalid');

    if (mustMatch() && (hasEight && hasLower && hasUpper && hasDigit && hasSpecial)) {
        document.getElementById('btnAccount').disabled = false;
    } else {
        document.getElementById('btnAccount').disabled = true;
    }
