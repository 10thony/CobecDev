        if (has) {
            $element.find(".fa-solid").removeClass("fa-solid fa-circle-xmark").removeClass('invalid').addClass('valid').addClass("fa-solid fa-circle-check");
            return true;
        }

        $element.find(".fa-solid").removeClass("fa-solid fa-circle-check").removeClass('valid').addClass('invalid').addClass("fa-solid fa-circle-xmark");
        return false;

    };

