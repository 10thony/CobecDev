{
  "name": "cobecium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun scripts/dev-with-convex.js",
    "dev:frontend": "vite --open",
    "dev:backend": "node scripts/convex-dev-wrapper.js",
    "dev:no-check": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "test": "vitest",
    "migrate:backup": "bun scripts/backup-mongodb.js backup",
    "migrate:restore": "bun scripts/backup-mongodb.js restore",
    "migrate:run": "bun scripts/run-migration.js",
    "migrate:full": "bun run migrate:backup && bun run migrate:run",
    "mongodb:setup-local": "bun scripts/setup-local-mongodb.js",
    "mongodb:test-local": "bun scripts/test-local-mongodb.js",
    "mongodb:migrate-atlas": "bun scripts/migrate-atlas-to-local.js",
    "mongodb:utils": "bun scripts/local-mongodb-utils.js",
    "convex:migrate:status": "bun scripts/migrate-to-convex.js --status",
    "convex:migrate:jobs": "bun scripts/migrate-to-convex.js --jobs",
    "convex:migrate:resumes": "bun scripts/migrate-to-convex.js --resumes",
    "convex:migrate:all": "bun scripts/migrate-to-convex.js --all",
    "convex:migrate:cleanup": "bun scripts/migrate-to-convex.js --cleanup",
    "migrate:workdemos": "bun scripts/migrate-workdemos-data.js",
    "migrate:workdemos:dry-run": "bun scripts/migrate-workdemos-data.js --dry-run",
    "migrate:workdemos:jobs": "bun scripts/migrate-workdemos-data.js --type jobs",
    "migrate:workdemos:resumes": "bun scripts/migrate-workdemos-data.js --type resumes",
    "migrate:workdemos:cobecadmins": "bun scripts/migrate-workdemos-data.js --type cobecadmins",
    "migrate:workdemos:employees": "bun scripts/migrate-workdemos-data.js --type employees",
    "migrate:workdemos:kfcpoints": "bun scripts/migrate-workdemos-data.js --type kfcpoints",
    "semantic:setup": "bun scripts/setup-semantic-search.js",
    "semantic:embeddings": "bun scripts/generate-embeddings.js",
    "semantic:embeddings:jobs": "bun scripts/generate-embeddings.js --job-postings",
    "semantic:embeddings:resumes": "bun scripts/generate-embeddings.js --resumes",
    "semantic:embeddings:kfc": "bun scripts/generate-embeddings.js --kfc-points",
    "embeddings:regenerate": "bun scripts/embedding-regeneration-agent.js",
    "embeddings:regenerate:dry-run": "set NODE_ENV=development && bun scripts/embedding-regeneration-agent.js --dry-run",
    "embeddings:validate": "bun scripts/embedding-regeneration-agent.js --validate-only",
    "embeddings:test": "bun scripts/test-embedding-regeneration-agent.js",
    "docker:convex:start": "docker compose -f convex-docker-compose.yml up -d",
    "docker:convex:stop": "docker compose -f convex-docker-compose.yml down",
    "docker:convex:status": "docker compose -f convex-docker-compose.yml ps",
    "docker:convex:logs": "docker compose -f convex-docker-compose.yml logs -f",
    "docker:full:start": "docker compose -f cobecdev-docker-compose.yml up -d",
    "docker:full:stop": "docker compose -f cobecdev-docker-compose.yml down",
    "docker:full:status": "docker compose -f cobecdev-docker-compose.yml ps",
    "docker:full:logs": "docker compose -f cobecdev-docker-compose.yml logs -f",
    "docker:full:build": "docker compose -f cobecdev-docker-compose.yml build",
    "check:convex": "bun scripts/check-convex.js",
    "convex:admin-key": "bun scripts/get-convex-admin-key.js",
    "import:resumes": "bun run scripts/importResumes.ts",
    "reset:ai-denied": "bun scripts/reset-ai-denied-links.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@anthropic-ai/sdk": "^0.54.0",
    "@clerk/backend": "^2.1.0",
    "@clerk/clerk-react": "^5.32.0",
    "@convex-dev/agent": "^0.3.2",
    "@convex-dev/workflow": "^0.3.4",
    "@google/generative-ai": "^0.2.1",
    "@puppeteer/browsers": "^2.11.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@uploadthing/react": "^7.3.3",
    "@vnedyalk0v/react19-simple-maps": "^1.2.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.6",
    "convex-helpers": "^0.1.108",
    "d3-force": "^3.0.0",
    "d3-geo": "^3.1.1",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.24.0",
    "fuse.js": "^7.1.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.515.0",
    "mammoth": "^1.9.1",
    "mongodb": "^6.3.0",
    "openai": "^5.3.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.3.31",
    "prismjs": "^1.30.0",
    "puppeteer": "^24.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.6.2",
    "react-syntax-highlighter": "^15.6.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.4",
    "vitest": "^3.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/d3-force": "^3.0.10",
    "@types/d3-geo": "^3.1.0",
    "@types/node": "^22.15.31",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "~10",
    "concurrently": "^9.2.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5"
  },
  "overrides": {
    "prismjs": "^1.30.0"
  }
}
