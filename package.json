{
  "name": "cobecium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "test": "vitest",
    "migrate:backup": "node scripts/backup-mongodb.js backup",
    "migrate:restore": "node scripts/backup-mongodb.js restore",
    "migrate:run": "node scripts/run-migration.js",
    "migrate:full": "npm run migrate:backup && npm run migrate:run",
    "mongodb:setup-local": "node scripts/setup-local-mongodb.js",
    "mongodb:test-local": "node scripts/test-local-mongodb.js",
    "mongodb:migrate-atlas": "node scripts/migrate-atlas-to-local.js",
    "mongodb:utils": "node scripts/local-mongodb-utils.js",
    "convex:migrate:status": "node scripts/migrate-to-convex.js --status",
    "convex:migrate:jobs": "node scripts/migrate-to-convex.js --jobs",
    "convex:migrate:resumes": "node scripts/migrate-to-convex.js --resumes",
    "convex:migrate:all": "node scripts/migrate-to-convex.js --all",
    "convex:migrate:cleanup": "node scripts/migrate-to-convex.js --cleanup",
    "migrate:workdemos": "node scripts/migrate-workdemos-data.js",
    "migrate:workdemos:dry-run": "node scripts/migrate-workdemos-data.js --dry-run",
    "migrate:workdemos:jobs": "node scripts/migrate-workdemos-data.js --type jobs",
    "migrate:workdemos:resumes": "node scripts/migrate-workdemos-data.js --type resumes",
    "migrate:workdemos:cobecadmins": "node scripts/migrate-workdemos-data.js --type cobecadmins",
    "migrate:workdemos:employees": "node scripts/migrate-workdemos-data.js --type employees",
    "migrate:workdemos:kfcpoints": "node scripts/migrate-workdemos-data.js --type kfcpoints",
    "semantic:setup": "node scripts/setup-semantic-search.js",
    "semantic:embeddings": "node scripts/generate-embeddings.js",
    "semantic:embeddings:jobs": "node scripts/generate-embeddings.js --job-postings",
    "semantic:embeddings:resumes": "node scripts/generate-embeddings.js --resumes",
    "semantic:embeddings:kfc": "node scripts/generate-embeddings.js --kfc-points",
    "embeddings:regenerate": "node scripts/embedding-regeneration-agent.js",
    "embeddings:regenerate:dry-run": "set NODE_ENV=development && node scripts/embedding-regeneration-agent.js --dry-run",
    "embeddings:validate": "node scripts/embedding-regeneration-agent.js --validate-only",
    "embeddings:test": "node scripts/test-embedding-regeneration-agent.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@clerk/backend": "^2.1.0",
    "@clerk/clerk-react": "^5.32.0",
    "@google/generative-ai": "^0.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "convex": "^1.24.8",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.515.0",
    "mammoth": "^1.9.1",
    "mongodb": "^6.3.0",
    "openai": "^5.3.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.3.31",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-syntax-highlighter": "^15.6.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "vitest": "^3.2.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.31",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "~10",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5"
  },
  "overrides": {
    "prismjs": "^1.30.0"
  }
}
